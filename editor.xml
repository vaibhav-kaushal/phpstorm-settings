<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="EFFECT_COLOR" value="2072dc" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bsnappy\b.*">
      <option name="FOREGROUND" value="b3317f" />
      <option name="FONT_TYPE" value="1" />
      <option name="ERROR_STRIPE_COLOR" value="e374d7" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bimportant\b.*">
      <option name="FOREGROUND" value="ff1807" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_COLOR" value="ffffff" />
      <option name="ERROR_STRIPE_COLOR" value="e31f5e" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
  </component>
</application>